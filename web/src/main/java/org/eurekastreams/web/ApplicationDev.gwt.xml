<module rename-to="eureka">
	<inherits name='com.google.gwt.user.User' />
	<inherits name="com.google.gwt.json.JSON" />
    <inherits name="com.google.gwt.resources.Resources" />
	<inherits name="org.eurekastreams.commons.ActionRPCService" />
	<inherits name="org.eurekastreams.server.Domain" />
	<inherits name='com.allen_sauer.gwt.dnd.gwt-dnd' />
    <inherits name='net.sf.gilead.Gilead4Gwt'/>
    <inherits name="com.google.gwt.logging.Logging"/>
    <inherits name='com.googlecode.gchart.GChart'/>
    <inherits name='com.google.gwt.widgetideas.GWTCanvas' />
    
    <set-property name="user.agent" value="safari"/>    
    <set-configuration-property name="CssResource.style" value="pretty"/>
	<set-property name="gwt.logging.popupHandler" value="DISABLED" />
    
	<entry-point class='org.eurekastreams.web.client.ui.pages.master.ApplicationEntryPoint' />
	<entry-point class='org.eurekastreams.web.client.ui.pages.master.ConnectEntryPoint' />
</module>
